<launch>
    <node name="tinker_voice_recognize" pkg="tinker_voice_recognize" type="recognizer.py" output="screen">
        <param name="hmm" value="$(find tinker_voice_recognize)/model/en-us/en-us" type="string"/>
        <param name="lm" value="$(find tinker_voice_recognize)/model/en-us/en-us.lm.bin" type="string"/>
        <param name="dict" value="$(find tinker_voice_recognize)/model/en-us/cmudict-en-us.dict" type="string"/>
    </node>
</launch>
